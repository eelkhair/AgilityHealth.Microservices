{
  "profiles": {
    "http": {
      "commandName": "Project",
      "launchBrowser": true,
      "launchUrl": "swagger",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "dotnetRunMessages": true,
      "applicationUrl": "http://localhost:5001"
    },
    "Docker": {
      "commandName": "Docker",
      "launchBrowser": true,
      "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}/swagger",
      "publishAllPorts": true
    },
    "dapr": {
      "commandName": "Executable",
      "workingDirectory": "$(ProjectDir)",
      "launchBrowser": true,
      "applicationUrl": "http://localhost:5001",
      "executablePath": "dapr",
      "commandLineArgs": "run --app-id ah-metadata-api --resources-path ../../../../Components/ --config ../../../../Config/config.yaml --app-port 5001 --dapr-http-port 50012 dotnet run"
    }
  },
  "$schema": "https://json.schemastore.org/launchsettings.json"
}